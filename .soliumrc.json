{
  "extends": "solium:recommended",
  "plugins": [
    "security"
  ],
  "rules": {
    "max-len": ["error", 150],
    "security/no-block-members": "off",
    "quotes": [
      "error",
      "double"
    ],
    "indentation": [
      "error",
      4
    ],
    "linebreak-style": [
      "off",
      "unix"
    ]
  }
}
