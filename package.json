{
  "name": "flow-protocol",
  "version": "0.0.1",
  "author": "Bryan Chen <bryan@laminar.one>",
  "private": true,
  "scripts": {
    "generate": "truffle compile && typechain --target truffle './build/**/*.json' && typechain --target web3-1.0.0 './build/**/*.json'",
    "postinstall": "yarn generate",
    "verify": "truffle run verify MoneyMarket FlowProtocol FlowToken LiquidityPool SimplePriceOracle --network kovan",
    "lint": "solium -d contracts/ && eslint --ext .js,.ts . && tsc --noEmit --pretty",
    "test": "truffle test",
    "ganache": "ganache-cli -p 9545 -m 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat' > /dev/null &",
    "stop": "sudo kill `sudo lsof -t -i:9545`"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^2.3.0",
    "truffle-plugin-verify": "^0.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "^4.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "ethlint": "^1.2.5",
    "ganache-cli": "^6.7.0",
    "openzeppelin-test-helpers": "^0.4.3",
    "truffle": "^5.0.39",
    "truffle-contract-size": "^1.0.1",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-typings": "^1.0.8",
    "ts-node": "^8.4.1",
    "typechain": "^0.3.21",
    "typescript": "^3.6.4"
  }
}
