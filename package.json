{
  "name": "@laminar/flow-protocol-ethereum",
  "files": [
    "artifacts"
  ],
  "version": "0.0.11",
  "author": "Bryan Chen <bryan@laminar.one>",
  "private": false,
  "scripts": {
    "generate": "yarn truffle compile && yarn typechain --target truffle-v5 \"./build/**/*.json\" && yarn typechain --target web3-v1 \"./build/**/*.json\"",
    "prepare": "yarn generate && cd subgraph && yarn && cross-env NETWORK=kovan yarn codegen",
    "verify": "cross-env-shell truffle run verify Proxy MoneyMarket SyntheticFlowProtocol SyntheticFlowToken SyntheticLiquidityPool MarginLiquidityPool SimplePriceOracle MarginFlowProtocol --network $NETWORK",
    "lint": "yarn solhint 'contracts/**/*.sol' && yarn eslint --ext .js,.ts subgraph && yarn tsc --noEmit --pretty",
    "test": "bash ./scripts/run_tests.sh",
    "test:debug": "yarn buidler test",
    "test:cucumber": "bash ./scripts/run_cucumber_tests.sh",
    "docify": "node ./scripts/docify.js",
    "graph-node": "docker-compose up",
    "deploy-local": "sh deploy-local.sh",
    "deploy": "cross-env-shell yarn truffle migrate --reset --network $NETWORK --skip-dry-run",
    "deploy:development": "cross-env NETWORK=development yarn deploy",
    "deploy:kovan": "cross-env NETWORK=kovan yarn deploy",
    "deploy:mainnet": "cross-env NETWORK=mainnet yarn deploy",
    "change-owner": "cross-env-shell NETWORK=$NETWORK yarn truffle exec dev-scripts/changeOwners.js --network $NETWORK",
    "change-owner:development": "cross-env NETWORK=development yarn change-owner",
    "change-owner:kovan": "cross-env NETWORK=kovan change-owner",
    "change-owner:mainnet": "cross-env NETWORK=mainnet change-owner",
    "upgrade-contracts:kovan": "cross-env-shell CONTRACT_NAME=$CONTRACT_NAME NETWORK=kovan yarn truffle exec dev-scripts/upgradeContract.js --network kovan"
  },
  "devDependencies": {
    "@chainlink/contracts": "0.0.5",
    "@codechecks/client": "^0.1.10",
    "@nomiclabs/buidler": "1.3.6",
    "@nomiclabs/buidler-truffle5": "1.3.4",
    "@nomiclabs/buidler-web3": "1.3.4",
    "@openzeppelin/contracts": "3.0.2",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/upgrades": "2.8.0",
    "@truffle/hdwallet-provider": "^1.1.0",
    "@typechain/truffle-v5": "^2.0.2",
    "@typechain/web3-v1": "^1.0.0",
    "@types/chai": "4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "14.0.13",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "bn.js": "^5.1.2",
    "chai": "^4.2.0",
    "chai-bn": "0.2.1",
    "cross-env": "^7.0.2",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-prettier": "3.1.4",
    "eth-gas-reporter": "0.2.17",
    "ethlint": "^1.2.5",
    "ganache-cli": "^6.9.1",
    "openzeppelin-test-helpers": "^0.5.1",
    "prettier": "2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.54",
    "solc": "0.6.10",
    "solhint": "^3.0.0",
    "solhint-plugin-prettier": "^0.0.4",
    "solidity-docgen": "0.5.3",
    "truffle": "5.1.30",
    "truffle-contract-size": "^1.0.1",
    "truffle-plugin-verify": "0.3.11",
    "ts-node": "8.10.2",
    "typechain": "2.0.0",
    "typescript": "3.9.5",
    "web3": "1.2.6",
    "web3-core": "1.2.6",
    "web3-eth-contract": "1.2.6"
  }
}
